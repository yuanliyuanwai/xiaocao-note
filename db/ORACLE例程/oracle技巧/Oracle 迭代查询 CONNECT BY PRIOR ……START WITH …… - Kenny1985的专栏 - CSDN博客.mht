	function addEventHandler(target, type, func) {
		if (target.addEventListener)
			target.addEventListener(type, func, false);
		else if (target.attachEvent)
			target.attachEvent("on" + type, func);
		else target["on" + type] =3D func;
	}
	addEventHandler(window, "load", function () {
		({
			anndelay: 5000,
			annst: 0,
			annstop: 0,
			annrowcount: 0,
			anncount: 0,
			annrows: [],
			annstatus: 0,
			announcementScroll: function () {
				var self =3D this;
				var annlis =3D self.annbody.getElementsByTagName("li");
				if (self.annstop) {
					self.annst =3D setTimeout(function () { self.announcementScroll(); =
}, self.anndelay);
					return;
				}
				if (!self.annst) {
					var lasttop =3D -1;
					for (var i =3D 0; i < annlis.length; i++) {

						if (lasttop !=3D annlis[i].offsetTop) {
							if (lasttop =3D=3D -1) {
								lasttop =3D 0;
							}
							self.annrows[self.annrowcount] =3D annlis[i].offsetTop - lasttop;
							self.annrowcount++;
						}
						lasttop =3D annlis[i].offsetTop;
					}

					if (self.annrows.length =3D=3D 1) {
						self.ann.onmouseover =3D self.ann.onmouseout =3D null;
					} else {
						self.annrows[self.annrowcount] =3D self.annrows[1];
						self.annbodylis.innerHTML +=3D self.annbodylis.innerHTML;
						self.annst =3D setTimeout(function () { self.announcementScroll(); =
}, self.anndelay);
					}
					self.annrowcount =3D 1;
					return;
				}

				if (self.annrowcount >=3D self.annrows.length) {
					self.annbody.scrollTop =3D 0;
					self.annrowcount =3D 1;
					self.annst =3D setTimeout(function () { self.announcementScroll(); =
}, self.anndelay);
				} else {
					self.anncount =3D 0;
					self.announcementScrollnext(self.annrows[self.annrowcount]);
				}
			},
			announcementScrollnext: function (time) {
				var self =3D this;
				self.annbody.scrollTop++;
				self.anncount++;
				if (self.anncount !=3D time) {
					self.annst =3D setTimeout(function () {
						self.announcementScrollnext(time);
					}, 10);
				} else {
					self.annrowcount++;
					self.annst =3D setTimeout(function () { self.announcementScroll(); =
}, self.anndelay);
				}
			},
			play: function () {
				var self =3D this;
				self.annbody =3D document.getElementById('annbody');
				if (!self.annbody) return;
				self.ann =3D document.getElementById('ann');
				self.annbodylis =3D document.getElementById('annbodylis');
				self.chal =3D document.getElementById('chal');
				if (!self.annbodylis.getElementsByTagName("li").length) return;
				self.chal.style.display =3D "";
				self.ann.onmouseover =3D function () { this.annstop =3D 1; };
				self.ann.onmouseout =3D function () { this.annstop =3D 0; };
				self.announcementScroll();
			}
		}).play();
	});
